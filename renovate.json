{
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["npm"],
  "pinVersions": false,
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "enabled": false
    },
    {
      "matchPackageNames": ["@snapshot-labs/snapshot.js"],
      "matchPackagePatterns": ["^@snapshot-labs/"],
      "groupName": "snapshot-labs packages",
      "enabled": true
    }
  ]
}
